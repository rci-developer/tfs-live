{
  "name": "algoritmika/product-visibility-by-user-role-for-woocommerce-pro",
  "description": "Display WooCommerce products by customer's user role.",
  "keywords": [
    "woo commerce",
    "woocommerce",
    "product",
    "visibility",
    "user role",
    "algoritmika",
    "wpcodefactory"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/algoritmika/product-visibility-by-user-role-for-woocommerce"
    }
  ],
  "authors": [
    {
      "name": "Tomas Anbinderis",
      "email": "anbinderis@gmail.com"
    },
    {
      "name": "Pablo Pacheco",
      "email": "pablo.sg.pacheco@gmail.com"
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "type": "wordpress-plugin",
  "require": {
    "php": ">=5.3.2",
    "composer/installers": ">=v1.2.0",
    "algoritmika/product-visibility-by-user-role-for-woocommerce": "dev-master"
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}": [
        "type:wordpress-plugin"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "includes/"
    ],
    "files": [
      "vendor/algoritmika/product-visibility-by-user-role-for-woocommerce/product-visibility-by-user-role-for-woocommerce.php"
    ]
  }
}